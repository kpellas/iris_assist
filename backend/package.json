{
  "name": "kelly-assistant-backend",
  "version": "1.0.0",
  "description": "Backend service for Kelly Assistant",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "test": "jest",
    "deploy": "npm run build && serverless deploy",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:seed": "tsx src/database/seed.ts",
    "db:reset": "psql -U kellypellas -d postgres -c 'DROP DATABASE IF EXISTS kelly_assistant' && psql -U kellypellas -d postgres -c 'CREATE DATABASE kelly_assistant' && npm run db:migrate"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.940.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-lambda": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-secrets-manager": "^3.940.0",
    "@aws-sdk/client-ssm": "^3.940.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "@langchain/community": "^0.0.10",
    "@langchain/openai": "^0.0.10",
    "@types/aws-lambda": "^8.10.125",
    "ask-sdk-core": "^2.14.0",
    "ask-sdk-model": "^1.40.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.5.0",
    "googleapis": "^128.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.0.200",
    "openai": "^4.20.0",
    "pg": "^8.11.3",
    "pgvector": "^0.2.0",
    "socket.io": "^4.6.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "serverless": "^3.37.0",
    "serverless-offline": "^13.3.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
